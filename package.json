{
  "name": "todos",
  "version": "0.0.0",
  "description": "The todos monorepo",
  "license": "UNLICENSED",
  "private": true,
  "repository": "git@github.com:vramework/workspace-starter.git",
  "author": "Yasser Fadl <yasser.fadl@gmail.com>",
  "type": "module",
  "workspaces": {
    "packages": [
      "packages/**",
      "apps/**",
      "backends/**"
    ]
  },
  "scripts": {
    "dbmigrate": "node --loader ts-node/esm scripts/db-migrate.ts",
    "vramework": "npx @vramework/cli all",
    "kysely": "DATABASE_URL=postgres://postgres:password@localhost/todos kysely-codegen --camel-case --schema public",
    "kysely-pure": "node --loader ts-node/esm scripts/kysely-pure.ts",
    "prebuild": "yarn run dbmigrate && yarn run kysely && yarn run kysely-pure && yarn run vramework && yarn workspaces foreach -p -A run prebuild",
    "verify": "yarn run tsc",
    "lint": "npx oxlint@latest",
    "prettier": "npx prettier . --write",
    "tsc": "yarn workspaces foreach -p -A run tsc",
    "test": "yarn workspaces foreach -p -A run test",
    "ncu": "yarn workspaces foreach -p -A run ncu -x '/.*chai.*/' -u",
    "ncu:vramework": "ncu -f '/@vramework/.*/' -u && yarn ncu -f '/@vramework/.*/' && yarn"
  },
  "devDependencies": {
    "@todos/functions": "workspace:0.0.0",
    "@vramework/cli": "^0.5.33",
    "kysely-codegen": "^0.16.6",
    "postgres-migrations": "^5.3.0",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6"
  },
  "lint-staged": {},
  "dependencies": {
    "eventemitter3": "^5.0.1",
    "pino": "^9.3.2"
  },
  "packageManager": "yarn@4.5.0",
  "engines": {
    "node": ">=18"
  },
  "resolutions": {
    "@vramework/cli": "portal:/Users/yasser/git/vramework/vramework-website/vramework/packages/cli",
    "@vramework/aws-services": "portal:/Users/yasser/git/vramework/vramework-website/vramework/packages/cloud/aws/aws-services",
    "@vramework/lambda": "portal:/Users/yasser/git/vramework/vramework-website/vramework/packages/cloud/aws/lambda",
    "@vramework/azure-functions": "portal:/Users/yasser/git/vramework/vramework-website/vramework/packages/cloud/azure/functions",
    "@vramework/core": "portal:/Users/yasser/git/vramework/vramework-website/vramework/packages/core",
    "@vramework/fetch": "portal:/Users/yasser/git/vramework/vramework-website/vramework/packages/fetch",
    "@vramework/schedule": "portal:/Users/yasser/git/vramework/vramework-website/vramework/packages/schedule",
    "@vramework/express-middleware": "portal:/Users/yasser/git/vramework/vramework-website/vramework/packages/servers/express/express-middleware",
    "@vramework/express": "portal:/Users/yasser/git/vramework/vramework-website/vramework/packages/servers/express/express-server",
    "@vramework/fastify-plugin": "portal:/Users/yasser/git/vramework/vramework-website/vramework/packages/servers/fastify/fastify-plugin",
    "@vramework/fastify": "portal:/Users/yasser/git/vramework/vramework-website/vramework/packages/servers/fastify/fastify-server",
    "@vramework/next": "portal:/Users/yasser/git/vramework/vramework-website/vramework/packages/servers/next",
    "@vramework/uws-handler": "portal:/Users/yasser/git/vramework/vramework-website/vramework/packages/servers/uws/uws-handler",
    "@vramework/uws": "portal:/Users/yasser/git/vramework/vramework-website/vramework/packages/servers/uws/uws-server",
